#include <stdio.h>

int main() {
    int x =10;
    int y = --x;
    int z = x++;
    printf("%d\n%d\n%d", x,y,z);
    return 0;
}

/* output:
10
9
9
*/

===========================
#include <stdio.h>

int main() {
      int x =10;
    int y = x++;// y=10, x=11
    int z = y++;// z=10, y=11
    printf("%d\n%d\n%d", x,y,z);
    return 0;
}
